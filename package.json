{
  "name": "@insurgent/conventional-changelog-preset",
  "description": "conventional-changelog preset",
  "version": "10.0.0",
  "author": "Pierre Vanduynslager",
  "maintainers": [
    "Pierre Cavin <me@sherlox.io>"
  ],
  "license": "MIT",
  "exports": {
    ".": "./index.js",
    "./release-rules": "./release-rules.js"
  },
  "type": "module",
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier ./**/*.{json,md,yml} --check",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write",
    "test": "cross-env NODE_OPTIONS=\"--loader=esmock --no-warnings\" c8 ava --no-worker-threads -v",
    "release": "semantic-release"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "ava": "6.2.0",
    "c8": "10.1.3",
    "conventional-changelog": "6.0.0",
    "cross-env": "7.0.3",
    "emoji-regex": "10.5.0",
    "eslint": "9.34.0",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-ava": "15.1.0",
    "eslint-plugin-prettier": "5.5.4",
    "esmock": "2.7.2",
    "execa": "9.6.0",
    "fs-extra": "11.3.1",
    "get-stream": "9.0.1",
    "p-each-series": "3.0.0",
    "prettier": "3.6.2",
    "semantic-release": "24.2.3",
    "stringz": "2.1.0",
    "tempy": "3.1.0"
  },
  "peerDependencies": {
    "@semantic-release/commit-analyzer": ">= 11.1.0",
    "@semantic-release/release-notes-generator": ">= 14.0.0",
    "conventional-changelog": ">= 6.0.0"
  },
  "peerDependenciesMeta": {
    "@semantic-release/release-notes-generator": {
      "optional": true
    },
    "@semantic-release/commit-analyzer": {
      "optional": true
    },
    "conventional-changelog": {
      "optional": true
    }
  },
  "homepage": "https://github.com/insurgent-lab/conventional-changelog-preset#readme",
  "bugs": {
    "url": "https://github.com/insurgent-lab/conventional-changelog-preset/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/insurgent-lab/conventional-changelog-preset.git"
  },
  "keywords": [
    "changelog",
    "conventional-changelog",
    "conventional-commits",
    "publish",
    "release",
    "semantic-release"
  ],
  "files": [
    "lib/*.js",
    "index.js",
    "release-rules.js",
    "types.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "15s"
  },
  "c8": {
    "include": [
      "lib/*.js",
      "index.js",
      "release-rules.js",
      "types.js"
    ],
    "check-coverage": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
